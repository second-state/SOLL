FROM ubuntu:20.04

RUN apt update \
    && apt install -y software-properties-common

RUN apt update \
    && apt install -y \
        llvm-10-dev \
        llvm-10-tools \
        liblld-10-dev \
        git \
        cmake \
        make \
        wget \
        python3-psutil \
        binaryen

RUN rm -rf /var/lib/apt/lists/*
